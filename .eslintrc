{
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "extends": ["eslint:recommended", "eslint-config-prettier"],
  "rules": {
    "semi": "off",
    "no-console": "off",
    "no-unused-vars": "warn",
    "no-undef": "error"
  },
  "env": {
    "browser": true,
    "jquery": true
  },
  "plugins": ["html"]
}
